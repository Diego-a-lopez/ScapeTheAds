<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ScapeTheAdds"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ScapeTheAdds">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="es.finders.scapetheadds.menu.login.Login" />
        <activity android:name="es.finders.scapetheadds.menu.nickname.Nickname" />
        <activity android:name="es.finders.scapetheadds.menu.home.Home" />
        <activity android:name="es.finders.scapetheadds.menu.level.Level" />
        <activity android:name="es.finders.scapetheadds.menu.levelselector.LevelSelector" />
        <activity android:name="es.finders.scapetheadds.menu.Victory.Victory" />
        <activity android:name="es.finders.scapetheadds.menu.Defeat.Defeat" />
        <activity android:name="es.finders.scapetheadds.menu.leaderboard.Leaderboard" />
        <activity android:name="es.finders.scapetheadds.menu.settings.SettingsActivity" />
        <activity android:name="es.finders.scapetheadds.menu.highscores.Highscores" />
    </application>

</manifest>